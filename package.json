    "jimp": "^0.22.12",
    "open-graph-scraper": "6.7.1"
  },
  "devDependencies": {
    "@types/node": "20.14.13",
    "gts": "5.3.1",
    "tsx": "4.16.5",
    "typescript": "5.5.4",
    "underscore-cli": "^0.2.19"
  },
  "typeRoots": [
    "./node_modules/@types",
    "./app/types"
