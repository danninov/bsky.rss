    "start": "tsx ./app/index.ts",
    "dev": "tsx --watch ./app/index.ts"
  },
  "dependencies": {
    "@atproto/api": "0.9.6",
    "@atproto/xrpc": "0.4.1",
    "axios": "1.6.7",
    "dotenv": "^16.4.2",
    "feedsub": "^0.7.8",
    "html-entities": "2.4.0",
    "open-graph-scraper": "6.4.0",
    "sharp": "0.33.2"
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "gts": "5.2.0",
    "tsx": "4.7.1",
    "typescript": "5.3.3",
    "underscore-cli": "^0.2.19"
  },
  "typeRoots": [
    "./node_modules/@types",
    "./app/types"
  ],
  "packageManager": "yarn@4.1.0"
