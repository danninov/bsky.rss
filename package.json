{
  "name": "bsky.rss",
  "version": "2.0.3",
  "description": "A configurable RSS poster for Bluesky",
  "main": "index.js",
  "repository": "https://github.com/milanmdev/bsky.rss.git",
  "author": "Milan Mehra <milan@milanm.org>",
  "license": "MIT",
  "scripts": {
    "build:dev": "docker rmi -f bsky.rss && docker build . -t bsky.rss",
